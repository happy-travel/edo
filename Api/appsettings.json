{
  "AllowedHosts": "*",
  "AmazonS3": {
    "Options": "edo/amazon-s3/documents",
    "ContractsS3FolderName": "contracts",
    "ImagesS3FolderName": "images"
  },
  "Authority": {
    "Options": "edo"
  },
  "ConnectionStrings": {
    "Edo": "Server={0};Port={1};Database=edo;Userid={2};Password={3}"
  },
  "CurrencyConverter": {
    "Options": "edo/currency-service"
  },
  "Edo": {
    "BankDetails": {
      "AccountDetails": {
        "AED": "edo/bank-details/accounts/aed",
        "EUR": "edo/bank-details/accounts/eur",
        "USD": "edo/bank-details/accounts/usd"
      },
      "Options": "edo/bank-details"
    },
    "Client": {
      "Options": "edo/client"
    },
    "Database": {
      "Options": "edo/connection-string"
    },
    "Email": {
      "AdministratorInvitationTemplateId": "adminInvitationTemplateId",
      "Administrators": "administrators",
      "AgentInvitationTemplateId": "invitationTemplateId",
      "ApiKey": "apiKey",
      "BookingCancelledTemplateId": "bookingCancelledTemplateId",
      "BookingFinalizedTemplateId": "bookingFinalizedTemplateId",
      "BookingDeadlineNotificationTemplateId": "bookingDeadlineNotificationTemplateId",
      "BookingInvoiceTemplateId": "bookingInvoiceTemplateId",
      "BookingVoucherTemplateId": "bookingVoucherTemplateId",
      "CcNotificationAddresses": "ccNotificationAddresses",
      "EdoPublicUrl": "edoPublicUrl",
      "ExternalPaymentsTemplateId": "externalPaymentsTemplateId",
      "KnownCustomerReceiptTemplateId": "knownCustomerReceiptTemplateId",
      "MasterAgentRegistrationTemplateId": "masterAgentRegistrationTemplateId",
      "Options": "edo/email",
      "PaymentLinkPaymentConfirmation": "unknownCustomerReceiptTemplateId",
      "RegularAgentRegistrationTemplateId": "regularAgentRegistrationTemplateId",
      "ReservationsBookingFinalizedTemplateId": "reservationsBookingFinalizedTemplateId",
      "ReservationsBookingCancelledTemplateId": "reservationsBookingCancelledTemplateId",
      "BookingSummaryTemplateId": "bookingSummaryTemplateId",
      "BookingAdministratorSummaryTemplateId": "bookingAdministratorSummaryTemplateId",
      "BookingAdministratorPaymentsSummaryTemplateId": "bookingPaymentsSummaryTemplateId",
      "SenderAddress": "senderAddress",
      "AdminCreditCardPaymentConfirmationTemplateId": "adminCreditCardPaymentConfirmationTemplateId",
      "AgentCreditCardPaymentConfirmationTemplateId": "agentCreditCardPaymentConfirmationTemplateId"
    },
    "Google": {
      "Endpoint": "https://maps.googleapis.com/maps/api/",
      "Options": "edo/google"
    },
    "Payfort": {
      "Options": "edo/payfort",
      "Urls": "edo/payfort/urls"
    }
  },
  "Jaeger": {
    "AgentHost": "JAEGER_AGENT_HOST",
    "AgentPort": "JAEGER_AGENT_PORT"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "HappyTravel.StdOutLogger.HttpContextLoggingMiddleware": "Information"
    },
    "Sentry": {
      "Endpoint": "HTDC_EDO_SENTRY_ENDPOINT"
    }
  },
  "PaymentLinks": {
    "Options": "edo/payment-links",
    "ServiceTypes": {
      "HTL": "Hotel Booking",
      "TRN": "Transfer",
      "CMS": "Custom service"
    },
    "Currencies": [
      "AED",
      "SAR",
      "USD"
    ],
    "Endpoint": "endpoint"
  },
  "Redis": {
    "Endpoint": "HTDC_REDIS_HOST"
  },
  "Suppliers": {
    "Options": "edo/data-providers"
  },
  "UserEvents": {
    "Elastic": "edo/elastic"
  }, 
  "Vault": {
    "Endpoint": "HTDC_VAULT_ENDPOINT",
    "Engine": "secrets",
    "Role": "edo",
    "Token": "HTDC_VAULT_TOKEN"
  }
}